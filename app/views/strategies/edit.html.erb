<h1>Изменить стратегию</h1>

<%= form_with scope: :strategy, url: :strategies, local: true do |form| %>
  <p>
    <%= form.label 'Название' %><br>
    <%= form.text_field 'title', autocomplete: 'title' %>
  </p>

  <p>
    <%= form.label 'Код стратегии' %><br>
    <%= form.text_area 'code', autocomplete: 'code' %>
  </p>

  <p>
    <%= form.submit 'Обновить'%>
  </p>
<% end %>

<%= link_to 'Вернуться', :back %>